(
    	($L2 0 $D1)
	($L1 0 $D1) 
	($L1 $B1 $D1)
	($L2 $B1 $D1)

	( $L2 $B2  $D2)    	 
    	( $L1 $B2  $D2) 
	( $L1  $B1  $D2)
	( $L2  $B1  $D2)

);

