// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../system/initialConditions"

phases (water air);

//transportModel Newtonian;
//nu nu [ 0 2 -1 0 0 0 0 ] $nuf;
//rho rho [ 1 -3 0 0 0 0 0 ] $rhof;

water
{
    transportModel  Newtonian;
    nu              nu [ 0 2 -1 0 0 0 0 ]  $nuf;
    rho             rho [ 1 -3 0 0 0 0 0 ] $rhof;
}

air
{
    transportModel  Newtonian;
    nu              nu [ 0 2 -1 0 0 0 0 ]  1.48e-05;
    rho             rho [ 1 -3 0 0 0 0 0 ] 1;
}

sigma           sigma [ 1 0 -2 0 0 0 0 ] 0.0;


