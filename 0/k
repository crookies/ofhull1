FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../system/initialConditions"

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform $knun;

boundaryField
{
    atmosphere
    {
        type            inletOutlet;
        inletValue      uniform $knun;
        value           uniform $knun;
    }
    inlet
    {
        type            fixedValue;
        value           uniform $knun;
    }
    outlet
    {
        type            zeroGradient;
    }
    Sym_bottom
    {
        type            symmetryPlane;
    }
    Sym_midPlane
    {
        type            symmetryPlane;
    }
    Sym_side
    {
        type            symmetryPlane;
    }
    Carena
    {
        type            kqRWallFunction;
        value           uniform 0.0025;
    }
}


