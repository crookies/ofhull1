FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../system/initialConditions"

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform $omegak;

boundaryField
{
    atmosphere
    {
        type            inletOutlet;
        inletValue      uniform $omegak;
        value           uniform $omegak;
    }
    inlet
    {
        type            fixedValue;
        value           uniform $omegak;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform $omegak;
        value           uniform $omegak;
    }
    Sym_bottom
    {
        type            symmetryPlane;
    }
    Sym_midPlane
    {
        type            symmetryPlane;
    }
    Sym_side
    {
        type            symmetryPlane;
    }
    Carena
    {
        type            omegaWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        beta1           0.075;
        value           uniform $omegak;
    }
}


